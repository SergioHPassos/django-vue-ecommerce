n        </div>\n      </div>\n    </nav>\n    <!-- navbar end -->\n\n    <!-- main content start -->\n    <section class=\"section\">\n      <router-view />\n    </section>\n    <!-- main content end -->\n\n    <!-- footer start -->\n    <footer class=\"footer\">\n      <p class=\"has-text-centered\">Copyright (c) 2022</p>\n    </footer>\n    <!-- footer end -->\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      showMobileMenu: false,\n      cart: {\n        items: [],\n      },\n    };\n  },\n  beforeCreate() {\n    this.$store.commit('initializeStore');\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n  },\n  computed: {\n    cartTotalLength() {\n      let totalLength = 0;\n\n      // calculated total items\n      for (let i = 0; i < this.cart.items.length; i++) {\n        totalLength += this.cart.items[i].quantity;\n      }\n\n      return totalLength;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n@import '../node_modules/bulma';\n</style>\n"],"sourceRoot":""}]}