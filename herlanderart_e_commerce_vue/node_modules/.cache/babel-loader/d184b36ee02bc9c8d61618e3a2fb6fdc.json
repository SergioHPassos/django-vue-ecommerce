tity(item) {\r\n      item.quantity += 1;\r\n\r\n      this.updateCart();\r\n    },\r\n    updateCart() {\r\n      localStorage.setItem('cart', JSON.stringify(this.$store.state.cart));\r\n    },\r\n    removeFromCart(item) {\r\n      this.$emit('removeFromCart', item);\r\n\r\n      this.updateCart();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}