;AAAA;AAAA;AAAA;AAAA;AACjB,qBAAI,CAACE,MAAL,CAAYC,MAAZ,CAAmB,cAAnB,EAAmC,IAAnC;;AAEMC,6BAHW,GAGK,KAAI,CAACC,MAAL,CAAYC,MAAZ,CAAmBF,aAHxB;AAIXG,4BAJW,GAII,KAAI,CAACF,MAAL,CAAYC,MAAZ,CAAmBC,YAJvB;AAAA;AAAA,uBAMXd,KAAI,CACPe,GADG,4BACqBJ,aADrB,cACsCG,YADtC,GAEHE,IAFG,CAEE,UAACC,QAAD,EAAc;AAClB,uBAAI,CAACb,OAAL,GAAea,QAAQ,CAACd,IAAxB;AAEAe,0BAAQ,CAACC,KAAT,GAAiB,KAAI,CAACf,OAAL,CAAaF,IAAb,GAAoB,iBAArC;AACD,iBANG,EAOHkB,KAPG,CAOG,UAACC,KAAD,EAAW;AAChBC,yBAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,iBATG,CANW;;AAAA;AAiBjB;AACA,qBAAI,CAACZ,MAAL,CAAYC,MAAZ,CAAmB,cAAnB,EAAmC,KAAnC;;AAlBiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBlB,KApBM;AAqBPc,aArBO,uBAqBK;AACV;AACA,UAAIC,KAAK,CAAC,KAAKpB,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAgB,CAA5C,EAA+C;AAC7C,aAAKA,QAAL,GAAgB,CAAhB;AACF,OAJU,CAMV;;;AACA,UAAMqB,IAAG,GAAI;AACXtB,eAAO,EAAE,KAAKA,OADH;AAEXC,gBAAQ,EAAE,KAAKA;AAFJ,OAAb,CAPU,CAYV;;AACA,WAAKI,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAAgCgB,IAAhC,EAbU,CAeV;;AACAzB,WAAK,CAAC;AACJ0B,eAAO,EAAE,mCADL;AAEJC,YAAI,EAAE,YAFF;AAGJC,mBAAW,EAAE,IAHT;AAIJC,oBAAY,EAAE,IAJV;AAKJC,gBAAQ,EAAE,IALN;AAMJC,gBAAQ,EAAE;AANN,OAAD,CAAL;AAQD;AA7CM;AAXI,CAAf","names":["axios","toast","name","data","product","quantity","mounted","getProduct","methods","$store","commit","category_slug","$route","params","product_slug","get","then","response","document","title","catch","error","console","log","addToCart","isNaN","item","message","type","dismissible","pauseOnHover","duration","position"],"sources":["C:\\Users\\shp36\\Documents\\Programming\\django-projects\\e-commerce\\herlanderart_e_commerce_vue\\src\\views\\Product.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-product\">\r\n    <div class=\"columns is-multiline\">\r\n      <!-- product image/description start -->\r\n      <div class=\"column is-7\">\r\n        <figure class=\"image mb-6\">\r\n          <img v-bind:src=\"product.get_image\" />\r\n        </figure>\r\n\r\n        <h1 class=\"title\">{{ product.name }}</h1>\r\n        <p>{{ product.description }}</p>\r\n      </div>\r\n      <!-- product image/description end -->\r\n\r\n      <!-- product information start -->\r\n      <div class=\"column is-4\">\r\n        <h2 class=\"subtitle\">Information</h2>\r\n\r\n        <p><strong>Price: </strong>${{ product.price }}</p>\r\n\r\n        <div class=\"field has-addons mt-6\">\r\n          <div class=\"control\">\r\n            <input type=\"number\" class=\"input\" min=\"1\" v-model=\"quantity\" />\r\n          </div>\r\n\r\n          <div class=\"control\">\r\n            <a class=\"button is-dark\" @click=\"addToCart\">Add To Cart</a>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- product information end -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { toast } from 'bulma-toast';\r\n\r\nexport default {\r\n  name: 'Product',\r\n  data() {\r\n    return {\r\n      product: {},\r\n      quantity: 1,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getProduct();\r\n  },\r\n  methods: {\r\n    async getProduct() {\r\n      this.$store.commit('setIsLoading', true);\r\n\r\n      const category_slug = this.$route.params.category_slug;\r\n      const product_slug = this.$route.params.product_slug;\r\n\r\n      await axios\r\n        .get(`/api/v1/products/${category_slug}/${product_slug}`)\r\n        .then((response) => {\r\n          this.product = response.data;\r\n\r\n          document.title = this.product.name + ' | HerlanderArt';\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      // await axios must finish before loading is done\r\n      this.$store.commit('setIsLoading', false);\r\n    },\r\n    addToCart() {\r\n      // ensure quanitity is 1, security\r\n      if (isNaN(this.quantity) || this.quantity < 1) {\r\n        this.quantity = 1;\r\n      }\r\n\r\n      // get item details\r\n      const item = {\r\n        product: this.product,\r\n        quantity: this.quantity,\r\n      };\r\n\r\n      // add to cart file\r\n      this.$store.commit('addToCart', item);\r\n\r\n      // popup alert for item being added to the cart\r\n      toast({\r\n        message: 'The product was added to the cart',\r\n        type: 'is-success',\r\n        dismissible: true,\r\n        pauseOnHover: true,\r\n        duration: 2000,\r\n        position: 'bottom-right',\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}