KAAZ,CAAkBJ,IAA9B;AACD,GAdY;AAebK,SAAO,EAAE;AACPC,kBADO,0BACQC,IADR,EACc;AACnB,WAAKP,IAAL,CAAUC,KAAV,GAAkB,KAAKD,IAAL,CAAUC,KAAV,CAAgBO,MAAhB,CAChB,UAACC,CAAD;AAAA,eAAOA,CAAC,CAACC,OAAF,CAAUC,EAAV,KAAiBJ,IAAI,CAACG,OAAL,CAAaC,EAArC;AAAA,OADgB,CAAlB;AAGD;AALM,GAfI;AAsBbC,UAAQ,EAAE;AACRC,mBADQ,6BACU;AAChB,aAAO,KAAKb,IAAL,CAAUC,KAAV,CAAgBa,MAAhB,CAAuB,UAACC,GAAD,EAAMC,MAAN,EAAiB;AAC7C,eAAQD,GAAE,IAAKC,MAAM,CAACC,QAAtB;AACD,OAFM,EAEJ,CAFI,CAAP;AAGD,KALO;AAMRC,kBANQ,4BAMS;AACf,aAAO,KAAKlB,IAAL,CAAUC,KAAV,CAAgBa,MAAhB,CAAuB,UAACC,GAAD,EAAMC,MAAN,EAAiB;AAC7C,eAAQD,GAAE,IAAKC,MAAM,CAACN,OAAP,CAAeS,KAAf,GAAuBH,MAAM,CAACC,QAA7C;AACD,OAFM,EAEJ,CAFI,CAAP;AAGD;AAVO;AAtBG,CAAf","names":["axios","CartItem","name","components","data","cart","items","mounted","$store","state","methods","removeFromCart","item","filter","i","product","id","computed","cartTotalLength","reduce","acc","curVal","quantity","cartTotalPrice","price"],"sources":["C:\\Users\\shp36\\Documents\\Programming\\django-projects\\e-commerce\\herlanderart_e_commerce_vue\\src\\views\\Cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-cart\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">Cart</h1>\r\n      </div>\r\n\r\n      <div class=\"column is-12 box\">\r\n        <table class=\"table is-fullwidth\" v-if=\"cartTotalLength\">\r\n          <thead>\r\n            <tr>\r\n              <th>Product</th>\r\n              <th>Price</th>\r\n              <th>Quantity</th>\r\n              <th>Total</th>\r\n              <th></th>\r\n            </tr>\r\n          </thead>\r\n\r\n          <tbody>\r\n            <CartItem\r\n              v-for=\"item in cart.items\"\r\n              v-bind:key=\"item.product.id\"\r\n              v-bind:initialItem=\"item\"\r\n              v-on:removeFromCart=\"removeFromCart\"\r\n            />\r\n          </tbody>\r\n        </table>\r\n\r\n        <p v-else>You don't have any products in your cart...</p>\r\n      </div>\r\n\r\n      <div class=\"column is-12 box\">\r\n        <h2 class=\"subtitle\">Summary</h2>\r\n        <strong>${{ cartTotalPrice.toFixed(2) }}</strong\r\n        >, {{ cartTotalLength }} items\r\n        <hr />\r\n        <router-link to=\"/cart/checkout\" class=\"button is-dark\"\r\n          >Proceed to checkout</router-link\r\n        >\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport CartItem from '@/components/CartItem';\r\n\r\nexport default {\r\n  name: 'Cart',\r\n  components: {\r\n    CartItem,\r\n  },\r\n  data() {\r\n    return {\r\n      cart: {\r\n        items: [],\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.cart = this.$store.state.cart;\r\n  },\r\n  methods: {\r\n    removeFromCart(item) {\r\n      this.cart.items = this.cart.items.filter(\r\n        (i) => i.product.id !== item.product.id\r\n      );\r\n    },\r\n  },\r\n  computed: {\r\n    cartTotalLength() {\r\n      return this.cart.items.reduce((acc, curVal) => {\r\n        return (acc += curVal.quantity);\r\n      }, 0);\r\n    },\r\n    cartTotalPrice() {\r\n      return this.cart.items.reduce((acc, curVal) => {\r\n        return (acc += curVal.product.price * curVal.quantity);\r\n      }, 0);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}