"C:/Users/shp36/Documents/Programming/django-projects/e-commerce/herlanderart_e_commerce_vue/src/App.vue","sourceRoot":"","sourcesContent":["<template>\n  <div id=\"wrapper\">\n    <!-- navbar start -->\n    <nav class=\"navbar is-dark\">\n      <div class=\"navbar-brand\">\n        <router-link to=\"/\" class=\"navbar-item\"\n          ><strong>HerlanderArt Shop</strong></router-link\n        >\n        <a\n          class=\"navbar-burger\"\n          aria-label=\"menu\"\n          aria-expanded=\"false\"\n          data-target=\"navbar-menu\"\n          @click=\"showMobileMenu = !showMobileMenu\"\n        >\n          <!-- stripes for the burger -->\n          <span aria-hidden=\"true\"></span>\n          <span aria-hidden=\"true\"></span>\n          <span aria-hidden=\"true\"></span>\n        </a>\n      </div>\n      <div\n        class=\"navbar-menu\"\n        id=\"navbar-menu\"\n        v-bind:class=\"{ 'is-active': showMobileMenu }\"\n      >\n        <div class=\"navbar-end\">\n          <router-link to=\"/trays\" class=\"navbar-item\">Accessories</router-link>\n\n          <div class=\"navbar-item\">\n            <div class=\"buttons\">\n              <router-link to=\"/log-in\" class=\"button is-light\"\n                >Log in</router-link\n              >\n              <router-link to=\"/cart\" class=\"button is-success\">\n                <span class=\"icon\"><i class=\"fas fa-shopping-cart\"></i></span>\n                <span>Cart ({{ cartTotalLength }})</span>\n              </router-link>\n            </div>\n          </div>\n        </div>\n      </div>\n    </nav>\n    <!-- navbar end -->\n\n    <!-- main content start -->\n    <section class=\"section\">\n      <router-view />\n    </section>\n    <!-- main content end -->\n\n    <!-- footer start -->\n    <footer class=\"footer\">\n      <p class=\"has-text-centered\">Copyright (c) 2022</p>\n    </footer>\n    <!-- footer end -->\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      showMobileMenu: false,\n      cart: {\n        items: [],\n      },\n    };\n  },\n  beforeCreate() {\n    this.$store.commit('initializeStore');\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n  },\n  computed: {\n    cartTotalLength() {\n      let totalLength = 0;\n\n      // calculated total items\n      for (let i = 0; i < this.cart.items.length; i++) {\n        totalLength += this.cart.items[i].quantity;\n      }\n\n      return totalLength;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n@import '../node_modules/bulma';\n</style>\n"]}]}